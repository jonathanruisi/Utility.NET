	<!-- MediaSlider Default Control Template -->
	<ControlTemplate TargetType="{x:Type controls:MediaSlider}" x:Key="MediaSliderDefaultTemplate">
		<Border Background="{TemplateBinding Background}"
				BorderBrush="{TemplateBinding BorderBrush}"
				BorderThickness="{TemplateBinding BorderThickness}"
				Padding="{TemplateBinding Padding}"
				SnapsToDevicePixels="True">
			<Grid>
				<Grid.RowDefinitions>
					<RowDefinition Height="*"/>
					<RowDefinition Height="{TemplateBinding InnerPaddingSize}"/>
					<RowDefinition Height="{TemplateBinding ZoomBarSize}"/>
				</Grid.RowDefinitions>

				<Canvas Grid.Row="0" ClipToBounds="False" Background="{TemplateBinding TickBarBackground}">
					<elements:TickBarAdvanced x:Name="PART_TickBar" Canvas.ZIndex="0"
											  Orientation="Horizontal" DecimalPrecision="8"
											  Width="{Binding RelativeSource={RelativeSource AncestorType=Panel}, Path=ActualWidth}"
											  Height="{Binding RelativeSource={RelativeSource AncestorType=Panel}, Path=ActualHeight}"
											  Minimum="{TemplateBinding ZoomStart}"
											  Maximum="{TemplateBinding ZoomEnd}"
											  TickAlignment="{TemplateBinding TickAlignment}"
											  OriginTickRelativeSize="{TemplateBinding OriginTickRelativeSize}"
											  OriginTickThickness="{TemplateBinding OriginTickThickness}"
											  OriginTickZIndex="{TemplateBinding OriginTickZIndex}"
											  OriginTickBrush="{TemplateBinding OriginTickBrush}"
											  MajorTickRelativeSize="{TemplateBinding MajorTickRelativeSize}"
											  MajorTickThickness="{TemplateBinding MajorTickThickness}"
											  MajorTickZIndex="{TemplateBinding MajorTickZIndex}"
											  MajorTickBrush="{TemplateBinding MajorTickBrush}"
											  MinorTickRelativeSize="{TemplateBinding MinorTickRelativeSize}"
											  MinorTickThickness="{TemplateBinding MinorTickThickness}"
											  MinorTickZIndex="{TemplateBinding MinorTickZIndex}"
											  MinorTickBrush="{TemplateBinding MinorTickBrush}"/>
					<ContentPresenter x:Name="PART_SelectionHighlight" Canvas.ZIndex="1"
									  ContentTemplate="{TemplateBinding SelectionHighlightElementTemplate}"/>
					<ContentPresenter x:Name="PART_SelectionStart" Canvas.ZIndex="2"
									  ContentTemplate="{TemplateBinding SelectionStartElementTemplate}"/>
					<ContentPresenter x:Name="PART_SelectionEnd" Canvas.ZIndex="2"
									  ContentTemplate="{TemplateBinding SelectionEndElementTemplate}"/>
					<ContentPresenter x:Name="PART_Position" Canvas.ZIndex="3"
									  ContentTemplate="{TemplateBinding PositionElementTemplate}"/>
				</Canvas>

				<Canvas Grid.Row="2" ClipToBounds="False" Background="{TemplateBinding ZoomBarBackground}">
					<ContentPresenter x:Name="PART_ZoomThumb" Canvas.ZIndex="0"
									  ContentTemplate="{TemplateBinding ZoomThumbElementTemplate}"/>
					<ContentPresenter x:Name="PART_ZoomStart" Canvas.ZIndex="1"
									  ContentTemplate="{TemplateBinding ZoomStartElementTemplate}"/>
					<ContentPresenter x:Name="PART_ZoomEnd" Canvas.ZIndex="1"
									  ContentTemplate="{TemplateBinding ZoomEndElementTemplate}"/>
				</Canvas>
			</Grid>
		</Border>
	</ControlTemplate>

	<!-- MediaSlider Default Style -->
	<Style TargetType="{x:Type controls:MediaSlider}">
		<Setter Property="BorderBrush" Value="Black"/>
		<Setter Property="BorderThickness" Value="0"/>
		<Setter Property="Margin" Value="0"/>
		<Setter Property="Padding" Value="0"/>
		<Setter Property="ZoomBarSize" Value="0.5*"/>
		<Setter Property="InnerPaddingSize" Value="2"/>
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="TickBarBackground" Value="#20000000"/>
		<Setter Property="ZoomBarBackground" Value="#400080FF"/>
		<Setter Property="FramesPerSecond" Value="30"/>
		<Setter Property="Minimum" Value="0"/>
		<Setter Property="Maximum" Value="1"/>
		<Setter Property="Position" Value="0"/>
		<Setter Property="SelectionStart" Value="{x:Null}"/>
		<Setter Property="SelectionEnd" Value="{x:Null}"/>
		<Setter Property="ZoomStart" Value="0"/>
		<Setter Property="ZoomEnd" Value="1"/>
		<Setter Property="TickAlignment" Value="Bottom"/>
		<Setter Property="PositionElementAlignment" Value="Top"/>
		<Setter Property="SelectionElementAlignment" Value="Top"/>
		<Setter Property="SelectionHighlightElementAlignment" Value="Middle"/>
		<Setter Property="PositionElementRelativeSize" Value="1.0"/>
		<Setter Property="SelectionElementRelativeSize" Value="0.667"/>
		<Setter Property="SelectionHighlightElementRelativeSize" Value="0.5"/>
		<Setter Property="OriginTickBrush" Value="Black"/>
		<Setter Property="OriginTickThickness" Value="4"/>
		<Setter Property="OriginTickRelativeSize" Value="1"/>
		<Setter Property="OriginTickZIndex" Value="0"/>
		<Setter Property="MajorTickBrush" Value="Black"/>
		<Setter Property="MajorTickThickness" Value="2"/>
		<Setter Property="MajorTickRelativeSize" Value="1"/>
		<Setter Property="MajorTickZIndex" Value="0"/>
		<Setter Property="MinorTickBrush" Value="Gray"/>
		<Setter Property="MinorTickThickness" Value="1"/>
		<Setter Property="MinorTickRelativeSize" Value="0.333"/>
		<Setter Property="MinorTickZIndex" Value="0"/>
		<Setter Property="PositionElementTemplate" Value="{StaticResource PositionTopAlignedDataTemplateDefault}"/>
		<Setter Property="SelectionStartElementTemplate" Value="{StaticResource SelectionStartDataTemplateDefault}"/>
		<Setter Property="SelectionEndElementTemplate" Value="{StaticResource SelectionEndDataTemplateDefault}"/>
		<Setter Property="SelectionHighlightElementTemplate" Value="{StaticResource SelectionHighlightDataTemplateDefault}"/>
		<Setter Property="ZoomStartElementTemplate" Value="{StaticResource ZoomStartDataTemplateDefault}"/>
		<Setter Property="ZoomEndElementTemplate" Value="{StaticResource ZoomEndDataTemplateDefault}"/>
		<Setter Property="ZoomThumbElementTemplate" Value="{StaticResource ZoomThumbDataTemplateDefault}"/>
		<Setter Property="Template" Value="{StaticResource MediaSliderDefaultTemplate}"/>
	</Style>
</ResourceDictionary>